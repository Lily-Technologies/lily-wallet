{
  "compilerOptions": {
    "outDir": "./compiled-ts/",
    "sourceMap": true,
    "strictNullChecks": true,
    "module": "esnext",
    "jsx": "react",
    "target": "es5",
    "allowJs": true,
    "baseUrl": ".",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "types": [
      "react",
      "jest"
    ]
  },
  "files": [
    "./src/utils/accountMap.ts"
  ],
  "include": [
    "src"
  ],
  "exclude": [
    "src/__tests__/*"
  ]
}
